{
  "name": "admin",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "build:docker": "run-p type-check \"build-only:docker\" --",
    "build-only:docker": "vite build --mode docker",
    "build:production": "run-p type-check \"build-only:production\" --",
    "build-only:production": "vite build --mode production",
    "build:nginx": "run-p type-check \"build-only:nginx\" --",
    "build-only:nginx": "VITE_BUILD_DIR=admin vite build --mode nginx"
  },
  "dependencies": {
    "@kangc/v-md-editor": "^2.3.18",
    "ant-design-vue": "^4.0.8",
    "axios": "^1.6.3",
    "dayjs": "^1.11.10",
    "echarts": "^5.5.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "prismjs": "^1.29.0",
    "qs": "^6.12.1",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@iconify-json/bi": "^1.1.23",
    "@iconify-json/fa6-brands": "^1.1.19",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "~5.3.0",
    "unocss": "^0.58.3",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.10",
    "vue-tsc": "^1.8.25"
  }
}
